<p id="notice"><%= notice %></p>
<table id="linktable">
  <thead>
    <tr>
      <th>Title</th>
      <!--<th>Url</th>-->
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>

    <% @links.each do |link| %>
      <tr>
        <td><a href=<%= link.url %> > <%= link.title %></a></td>
        <!--<td><%= link.url %></td>-->
          <td><%= link_to 'Show', link %></td>
       </tr>
    <% end %>

  </tbody>
</table>

<br>


